
{
    "swagger":"2.0",
    "info":{
        "version":"1.0-SNAPSHOT",
        "title":"JaxrsJson"
    },
    "host":"example.com",
    "basePath":"/api",
    "schemes":[
        "http"
    ],
    "paths":{
        "/profile/add":{
            "post":{
                "description":"プロフィールを保存.  \n\n プロフィールの内容を保存します。\n 保存する内容は下記になります。\n - 名前\n - タイプ\n - 年齢",
                "consumes":[
                    "application/json"
                ],
                "produces":[
                ],
                "parameters":[
                    {
                        "name":"body",
                        "in":"body",
                        "required":true,
                        "schema":{
                            "$ref":"#/definitions/Profile"
                        },
                        "description":"プロフィール情報"
                    }
                ],
                "responses":{
                    "200":{
                        "description":"OK",
                        "headers":{
                        }
                    }
                }
            }
        },
        "/profile/get":{
            "get":{
                "description":"プロフィールのリスト表示.",
                "consumes":[
                ],
                "produces":[
                    "application/json"
                ],
                "parameters":[
                ],
                "responses":{
                    "200":{
                        "description":"OK",
                        "headers":{
                        },
                        "schema":{
                            "$ref":"#/definitions/ProfileResult"
                        }
                    }
                }
            }
        }
    },
    "definitions":{
        "Name":{
            "properties":{
                "first":{
                    "type":"string"
                },
                "last":{
                    "type":"string"
                }
            }
        },
        "Profile":{
            "properties":{
                "age":{
                    "type":"integer"
                },
                "name":{
                    "$ref":"#/definitions/Name"
                },
                "type":{
                    "type":"string",
                    "enum":[
                        "VETERAN",
                        "YOUTH"
                    ]
                }
            }
        },
        "ProfileResult":{
            "properties":{
                "profiles":{
                    "type":"array",
                    "items":{
                        "$ref":"#/definitions/Profile"
                    }
                },
                "summary":{
                    "$ref":"#/definitions/Summary"
                }
            }
        },
        "Summary":{
            "properties":{
                "totalCount":{
                    "type":"integer"
                },
                "veteranCounts":{
                    "type":"integer"
                },
                "youthCounts":{
                    "type":"integer"
                }
            }
        }
    }
}